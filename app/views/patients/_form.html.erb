<%= form_for [@hospital, @patient] do |f| %>
  <%= f.label :first %>
  <%= f.text_field :first %>
  <br>
  <%= f.label :last %>
  <%= f.text_field :last %>
  <br>
  <%= f.label :birth %>
  <%= f.text_field :birth %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <br>
  <%= f.label :blood %>
  <%= f.text_field :blood %>
  <br>
  <%= f.submit %>
  |
  <%= link_to "Cancel", hospital_path(@hospital) %>
  <% end %>
